{
  "name": "gamesys-digital-acquisition",
  "version": "1.0.0",
  "description": "Gamesys Digital Acquisition Landing Page",
  "author": "Digital Acquisition Services Team",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && npm run copy:config",
    "copy:config": "cp -r static/marketing/admin/config.yml dist/marketing/",
    "test": "jest",
    "test:unit": "npm test -- --watch"
  },
  "dependencies": {
    "@nuxtjs/markdownit": "^1.2.9",
    "axios": "^0.19.2",
    "frontmatter-markdown-loader": "^3.4.3",
    "jquery": "^3.5.1",
    "markdown-it": "^10.0.0",
    "nuxt": "^2.0.0",
    "nuxt-lazy-load": "^1.2.4",
    "save": "^2.4.0",
    "vue-slick": "^1.1.15",
    "vue-slick-carousel": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@vue/test-utils": "^1.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^5.0.0",
    "sass-jest": "^0.1.7",
    "sass-loader": "^10.0.5",
    "seed-barista": "^1.1.1",
    "vue-jest": "^3.0.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "scss"
    ],
    "transform": {
      "^.+\\.scss$": "sass-jest"
    },
    "testRegex": "__tests__/.*\\.(js|scss)$"
  }
